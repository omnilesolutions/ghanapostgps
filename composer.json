{
    "name": "omnilesolutions/ghanapostgps",
    "description": "PHP package for connecting with GhanaPostGPS",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "phpseclib/phpseclib": "^2.0",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "omnilesolutions",
            "email": "bright@omnile.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "GhanaPostGPS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
